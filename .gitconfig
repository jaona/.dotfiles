[user]
	name = Jaona Ramahaleo
	email = jaona.ramahaleo@gmail.com

[alias]
  co = checkout
  cp = cherry-pick
  ci = commit
  st = status
  br = branch

[color]
	ui = true
	[color "branch"]
		current = yellow reverse
		local = yellow
		remote = green
	[color "diff"]
		meta = yellow
		frag = magenta
		old = red
		new = green
		whitespace = red reverse
	[color "status"]
		added = yellow
		changed = green
		untracked = cyan

[core]
	autocrlf=input
	excludesfile=/Users/jaona/.gitignore
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol

[credential]
	helper = cache

[diff]
	algorithm = patience

[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
